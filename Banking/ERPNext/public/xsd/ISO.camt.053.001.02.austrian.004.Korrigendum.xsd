					<xd:sequence>
						<xd:choice>
							<xd:element name="Qty" type="_:FinancialInstrumentQuantityChoice">
								<xd:annotation>
									<xd:documentation xml:lang="de">Gehandelte Menge</xd:documentation>
									<xd:documentation xml:lang="en">Dealt quantity</xd:documentation>
								</xd:annotation>
							</xd:element>
						</xd:choice>
					</xd:sequence>
